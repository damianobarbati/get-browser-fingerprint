{
  "compilerOptions": {
    "module": "nodenext",
    "moduleResolution": "node",
    "target": "esnext",
    "lib": [
      "ES2022",
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "forceConsistentCasingInFileNames": true,
    "skipLibCheck": true,
    "strict": true,
    "sourceMap": true,
    "types": ["vitest/globals"]
  }
}
